{
    "name": "titus/dolmen",
    "description": "website responsive",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Titus\\Dolmen\\": "composer/",
            "Titus\\Dolmen\\Tests\\": "test/"
        }
    },
    "authors": [
        {
            "name": "titus"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
